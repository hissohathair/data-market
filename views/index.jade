extends layout

block content
	div(class='wrapper')
	header
	div(class='container', name='queryTermForm')
		form(method='get', role='form', action='timeline')
			div(class='form-group')
				label(for='queryTerm') Query term
				input(type='text', class='form-control', id='queryTerm', name='term', placeholder='Enter query term')
			button(type='submit', class='btn btn-default') Submit
	footer

